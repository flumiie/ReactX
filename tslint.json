{
  "extends": [
    "tslint:recommended",
    "tslint:latest",
    "tslint-react",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules":
  {
    "max-line-length":
    {
      "options": [80]
    },
    "one-line": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-semicolon": true,
    "ordered-imports": [true]
  }
}